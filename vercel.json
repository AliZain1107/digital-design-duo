{
  "version": 2,
  "builds": [
    {
      "src": "api/[[...slug]].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/_static/(.*)",
      "dest": "/dist/client/_static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/[[...slug]].js"
    }
  ],
  "functions": {
    "api/[[...slug]].js": {
      "includeFiles": "dist/**"
    }
  }
}